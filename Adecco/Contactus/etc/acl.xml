<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
             <resource id="Magento_Backend::admin">
                <resource id="Adecco_Contactus::base" title="Adecco">
                    <resource id="Adecco_Contactus::contactus" title="Contactus" sortOrder="30"/>
                </resource>
            </resource>
        </resources>
    </acl>
</config>